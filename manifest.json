{
  "manifest_version": 2,
  "name": "Scourgify",
  "description": "Scourgify",
  "version": "1.0",

  "icons": { 
    "48": "icon.png"
   },

  "permissions": [
    "tabs"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "browser_action": {
    "default_title": "This is a test",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}